= render 'posts/header_top'

.contents
  %h2 MY PAGE
  .content

    %p.name= @name
    - if @posts
      %ul.content-lists
        - @posts.each do |post|
          %li.content-list
            = link_to image_tag(post.image.url, class: 'post-image'), post_path(post)
            %p.post-title= post.title
